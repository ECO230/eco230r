library(eco230r)
#Load sample data for a paired samples t-test.
pst_data <- data.frame(readRDS('data/t_wide.Rda'))
#Generate a model for a paired samples t test comparing the black_tea variable to the green_tea variable. This will generate output for a two tailed test .
pst <- analyze(pst_data$black_tea, pst_data$green_tea,analysis = 'pst')
#The list of results that is returned.
pst
pst <- analyze(pst_data$black_tea, pst_data$green_tea,analysis = 'pst', tails = 1)
#The list of results that is returned.
pst
#Load sample data for one sample t-test and independent t-test.
idt_data <- data.frame(readRDS('data/t_tall.Rda'))
#Generate a model for a one sample t-test comparing average number of scones consumed to a hypothesized value of 44.  This will generate output for a one tailed test.
ost <- analyze(idt_data$scones, analysis = 'ost',tails = 1, mu = 44)
#The list of results that is returned.
ost
idt_formula <- idt_data$scones ~ idt_data$tea
idt <- analyze(idt_formula, idt_data, analysis = 'idt')
